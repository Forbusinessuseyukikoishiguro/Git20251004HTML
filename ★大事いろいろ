#ã‚ªãƒ¼ãƒãƒ¼ãƒ©ã‚¤ãƒ‰
# class Parent:
#     def greet(self):
#         print("ã“ã‚“ã«ã¡ã¯ã€è¦ªã‚¯ãƒ©ã‚¹ã§ã™ï¼")

# class Child(Parent):
#     def greet(self):  # è¦ªã® greet ã‚’ä¸Šæ›¸ã
#         print("ã“ã‚“ã«ã¡ã¯ã€å­ã‚¯ãƒ©ã‚¹ã§ã™ï¼")

# c = Child()
# c.greet()

class Parent:
    def greet(self):
        print("ã“ã‚“ã«ã¡ã¯ã€è¦ªã‚¯ãƒ©ã‚¹ã§ã™ï¼")

class Child(Parent):
    def greet(self):
        print("å­ã‚¯ãƒ©ã‚¹ã®æŒ¨æ‹¶å‰å‡¦ç†")
        super().greet()  # è¦ªã‚¯ãƒ©ã‚¹ã®ãƒ¡ã‚½ãƒƒãƒ‰ã‚’å‘¼ã¶
        print("å­ã‚¯ãƒ©ã‚¹ã®æŒ¨æ‹¶å¾Œå‡¦ç†")

c = Child()
c.greet()


ï¼ï¼ï¼ï¼ï¼ï¼ï¼ï¼ï¼ï¼ï¼ï¼ï¼ï¼ï¼ï¼ï¼ï¼ï¼ï¼ï¼ï¼ï¼ï¼ï¼ï¼ï¼ï¼ï¼ï¼
# ====================================
# ãµã‚ãµã‚å¤§ç¦åº—ã‚·ãƒªãƒ¼ã‚ºï¼šã‚¯ãƒ©ã‚¹ã¨ç¶™æ‰¿ã®ä¾‹
# Googleã‚¹ã‚¿ã‚¤ãƒ«Docstringä»˜ã
# ====================================

# â‘  è¦ªå­ã‚¯ãƒ©ã‚¹ï¼ˆå˜ä¸€ç¶™æ‰¿ï¼‰
class Parent:
    """è¦ªã‚¯ãƒ©ã‚¹ã®ä¾‹"""
    def greet(self):
        """è¦ªã‚¯ãƒ©ã‚¹ã®æŒ¨æ‹¶ãƒ¡ã‚½ãƒƒãƒ‰"""
        print("ã“ã‚“ã«ã¡ã¯ï¼ç§ã¯è¦ªã‚¯ãƒ©ã‚¹ã§ã™ã€‚")

class Child(Parent):
    """è¦ªã‚¯ãƒ©ã‚¹ã‚’ç¶™æ‰¿ã—ãŸå­ã‚¯ãƒ©ã‚¹"""
    def greet_child(self):
        """å­ã‚¯ãƒ©ã‚¹å°‚ç”¨ã®æŒ¨æ‹¶"""
        print("ç§ã¯å­ã‚¯ãƒ©ã‚¹ã§ã™ï¼")

# ãƒ†ã‚¹ãƒˆ
c = Child()
c.greet()        # è¦ªã®ãƒ¡ã‚½ãƒƒãƒ‰
c.greet_child()  # å­ã®ãƒ¡ã‚½ãƒƒãƒ‰


# â‘¡ éšå±¤ç¶™æ‰¿ï¼ˆè¦ªâ†’å­â†’å­«ï¼‰
class GrandParent:
    """ç¥–çˆ¶æ¯ã‚¯ãƒ©ã‚¹"""
    def say(self):
        """ç¥–çˆ¶æ¯ã®æŒ¨æ‹¶"""
        print("ç¥–çˆ¶æ¯ã‚¯ãƒ©ã‚¹ã§ã™ã€‚")

class Parent2(GrandParent):
    """è¦ªã‚¯ãƒ©ã‚¹"""
    def say_parent(self):
        print("è¦ªã‚¯ãƒ©ã‚¹ã§ã™ã€‚")

class Child2(Parent2):
    """å­ã‚¯ãƒ©ã‚¹"""
    def say_child(self):
        print("å­ã‚¯ãƒ©ã‚¹ã§ã™ã€‚")

# ãƒ†ã‚¹ãƒˆ
c2 = Child2()
c2.say()
c2.say_parent()
c2.say_child()


# â‘¢ å¤šé‡ç¶™æ‰¿ï¼ˆè¤‡æ•°ã®è¦ªã‚’æŒã¤ï¼‰
class A:
    def hello(self):
        print("Aã‚¯ãƒ©ã‚¹ã‹ã‚‰ã“ã‚“ã«ã¡ã¯")

class B:
    def hello(self):
        print("Bã‚¯ãƒ©ã‚¹ã‹ã‚‰ã“ã‚“ã«ã¡ã¯")

class C(A, B):
    """Aã¨Bã‚’ç¶™æ‰¿ï¼ˆå·¦å´å„ªå…ˆï¼‰"""
    pass

c3 = C()
c3.hello()  # AãŒå„ªå…ˆã•ã‚Œã‚‹


# â‘£ __init__ï¼ˆåˆæœŸåŒ–ï¼‰
class Shop:
    """ãŠåº—ã‚¯ãƒ©ã‚¹ã®ä¾‹"""
    def __init__(self, name):
        """
        ã‚¤ãƒ³ã‚¹ã‚¿ãƒ³ã‚¹ç”Ÿæˆæ™‚ã«åå‰ã‚’åˆæœŸåŒ–

        Args:
            name (str): ãŠåº—ã®åå‰
        """
        self.name = name
        print(f"{self.name}ã‚’é–‹åº—ã—ã¾ã—ãŸï¼")

shop = Shop("ãµã‚ãµã‚å¤§ç¦åº—")


# â‘¤ super()ï¼ˆè¦ªã®å‡¦ç†ã‚’å‘¼ã¶ï¼‰
class ParentInit:
    """è¦ªã‚¯ãƒ©ã‚¹ã®åˆæœŸåŒ–"""
    def __init__(self):
        print("è¦ªã‚¯ãƒ©ã‚¹ã®åˆæœŸåŒ–")

class ChildInit(ParentInit):
    """å­ã‚¯ãƒ©ã‚¹ã®åˆæœŸåŒ–"""
    def __init__(self):
        super().__init__()  # è¦ªã®åˆæœŸåŒ–ã‚’å‘¼ã¶
        print("å­ã‚¯ãƒ©ã‚¹ã®åˆæœŸåŒ–")

c_init = ChildInit()


# â‘¦ ç‰¹æ®Šãƒ¡ã‚½ãƒƒãƒ‰
class Daifuku:
    """å¤§ç¦ã‚¯ãƒ©ã‚¹"""
    def __init__(self, flavor):
        """
        Args:
            flavor (str): å¤§ç¦ã®å‘³
        """
        self.flavor = flavor

    def __str__(self):
        """æ–‡å­—åˆ—è¡¨ç¤º"""
        return f"ğŸ¡ {self.flavor}å¤§ç¦ã§ã™"

d = Daifuku("ã‚ã‚“ã“")
print(d)


# â‘§ ã‚¯ãƒ©ã‚¹å¤‰æ•°ã¨â‘¨ ã‚¤ãƒ³ã‚¹ã‚¿ãƒ³ã‚¹å¤‰æ•°
