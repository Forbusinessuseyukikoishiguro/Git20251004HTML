print("\n" + "="*70)
print("ã€ç¬¬3éƒ¨ã€‘å¤šé‡ç¶™æ‰¿ - è¤‡æ•°ã®è¦ªã‚¯ãƒ©ã‚¹ã‹ã‚‰æ©Ÿèƒ½ã‚’çµ„ã¿åˆã‚ã›")
print("="*70)

# --------------------------------------------------
# 4ã¤ã®ç‹¬ç«‹ã—ãŸè¦ªã‚¯ãƒ©ã‚¹ã‚’å®šç¾©
# --------------------------------------------------

# è¦ªã‚¯ãƒ©ã‚¹A: åŸºæœ¬åº—èˆ—æ©Ÿèƒ½
class ShopBase:
    """
    åº—èˆ—ã®åŸºæœ¬æ©Ÿèƒ½ï¼ˆå¤šé‡ç¶™æ‰¿ç”¨ã®è¦ªAï¼‰
    
    å½¹å‰²: åº—èˆ—ã¨ã—ã¦ã®åŸºæœ¬çš„ãªè²©å£²æ©Ÿèƒ½
    """
    
    def __init__(self, owner_name, stock):
        """
        ã‚³ãƒ³ã‚¹ãƒˆãƒ©ã‚¯ã‚¿ï¼ˆè¦ªAï¼‰
        
        å¼•æ•°:
            owner_name (str): åº—é•·å
            stock (int): åœ¨åº«æ•°
        """
        self.owner_name = owner_name
        self.stock = stock
        self.sold = 0
        print(f"ğŸª åº—èˆ—æ©Ÿèƒ½: {owner_name}åº—é•·")
    
    def sell(self, quantity):
        """åŸºæœ¬çš„ãªè²©å£²å‡¦ç†"""
        if quantity > self.stock:
            print(f"âŒ åœ¨åº«ä¸è¶³")
            return False
        self.stock -= quantity
        self.sold += quantity
        print(f"ğŸ’° {quantity}å€‹è²©å£²")
        return True


# è¦ªã‚¯ãƒ©ã‚¹B: ã‚ªãƒ³ãƒ©ã‚¤ãƒ³æ©Ÿèƒ½
class OnlineServiceMixin:
    """
    ã‚ªãƒ³ãƒ©ã‚¤ãƒ³æ³¨æ–‡æ©Ÿèƒ½ï¼ˆå¤šé‡ç¶™æ‰¿ç”¨ã®è¦ªBï¼‰
    
    å½¹å‰²: ã‚ªãƒ³ãƒ©ã‚¤ãƒ³ã§ã®æ³¨æ–‡å—ä»˜æ©Ÿèƒ½
    
    Mixin: æ©Ÿèƒ½è¿½åŠ å°‚ç”¨ã®ã‚¯ãƒ©ã‚¹
           å˜ç‹¬ã§ã¯ä½¿ã‚ãšã€ä»–ã®ã‚¯ãƒ©ã‚¹ã¨çµ„ã¿åˆã‚ã›ã¦ä½¿ã†
    """
    
    def __init__(self):
        """
        ã‚³ãƒ³ã‚¹ãƒˆãƒ©ã‚¯ã‚¿ï¼ˆè¦ªBï¼‰
        
        æ³¨æ„: å¼•æ•°ãªã—ï¼ˆæ©Ÿèƒ½ã ã‘ã‚’æä¾›ï¼‰
        """
        self.online_orders = 0  # ã‚ªãƒ³ãƒ©ã‚¤ãƒ³æ³¨æ–‡ã®ç´¯è¨ˆ
        print(f"ğŸŒ ã‚ªãƒ³ãƒ©ã‚¤ãƒ³æ©Ÿèƒ½: ã‚·ã‚¹ãƒ†ãƒ èµ·å‹•")
    
    def receive_online_order(self, customer, quantity):
        """
        ã‚ªãƒ³ãƒ©ã‚¤ãƒ³æ³¨æ–‡ã‚’å—ã‘ä»˜ã‘ã‚‹
        
        å¼•æ•°:
            customer (str): é¡§å®¢å
            quantity (int): æ³¨æ–‡å€‹æ•°
        """
        self.online_orders += 1
        print(f"ğŸŒ ã‚ªãƒ³ãƒ©ã‚¤ãƒ³æ³¨æ–‡ #{self.online_orders}")
        print(f"   é¡§å®¢: {customer}æ§˜ã€æ•°é‡: {quantity}å€‹")


# è¦ªã‚¯ãƒ©ã‚¹C: é…é”æ©Ÿèƒ½
class DeliveryServiceMixin:
    """
    é…é”ã‚µãƒ¼ãƒ“ã‚¹æ©Ÿèƒ½ï¼ˆå¤šé‡ç¶™æ‰¿ç”¨ã®è¦ªCï¼‰
    
    å½¹å‰²: å•†å“ã®é…é”æ©Ÿèƒ½
    """
    
    def __init__(self):
        """ã‚³ãƒ³ã‚¹ãƒˆãƒ©ã‚¯ã‚¿ï¼ˆè¦ªCï¼‰"""
        self.deliveries = 0  # é…é”ã®ç´¯è¨ˆå›æ•°
        print(f"ğŸšš é…é”æ©Ÿèƒ½: ã‚µãƒ¼ãƒ“ã‚¹é–‹å§‹")
    
    def deliver(self, address):
        """
        å•†å“ã‚’é…é”ã™ã‚‹
        
        å¼•æ•°:
            address (str): é…é”å…ˆä½æ‰€
        """
        self.deliveries += 1
        print(f"ğŸšš é…é” #{self.deliveries}: {address}ã¸é…é”ä¸­")


# è¦ªã‚¯ãƒ©ã‚¹D: ä¼šå“¡æ©Ÿèƒ½
class MembershipServiceMixin:
    """
    ä¼šå“¡ã‚µãƒ¼ãƒ“ã‚¹æ©Ÿèƒ½ï¼ˆå¤šé‡ç¶™æ‰¿ç”¨ã®è¦ªDï¼‰
    
    å½¹å‰²: ä¼šå“¡ã®ç™»éŒ²ã¨ç®¡ç†
    """
    
    def __init__(self):
        """ã‚³ãƒ³ã‚¹ãƒˆãƒ©ã‚¯ã‚¿ï¼ˆè¦ªDï¼‰"""
        self.members = []  # ä¼šå“¡åã®ãƒªã‚¹ãƒˆ
        print(f"ğŸ’³ ä¼šå“¡æ©Ÿèƒ½: ã‚·ã‚¹ãƒ†ãƒ èµ·å‹•")
    
    def register_member(self, name):
        """
        ä¼šå“¡ã‚’ç™»éŒ²ã™ã‚‹
        
        å¼•æ•°:
            name (str): ä¼šå“¡å
        """
        # append() = ãƒªã‚¹ãƒˆã«è¦ç´ ã‚’è¿½åŠ ã™ã‚‹ãƒ¡ã‚½ãƒƒãƒ‰
        self.members.append(name)
        # len() = ãƒªã‚¹ãƒˆã®é•·ã•ã‚’å–å¾—ã™ã‚‹é–¢æ•°
        print(f"ğŸ’³ {name}æ§˜ã‚’ä¼šå“¡ç™»éŒ²ï¼ˆä¼šå“¡æ•°: {len(self.members)}åï¼‰")
    
    def is_member(self, name):
        """
        ä¼šå“¡ã‹ã©ã†ã‹ãƒã‚§ãƒƒã‚¯
        
        å¼•æ•°:
            name (str): ãƒã‚§ãƒƒã‚¯ã™ã‚‹åå‰
        
        æˆ»ã‚Šå€¤:
            bool: ä¼šå“¡ãªã‚‰True
        """
        # inæ¼”ç®—å­ = ãƒªã‚¹ãƒˆã«è¦ç´ ãŒå«ã¾ã‚Œã¦ã„ã‚‹ã‹ãƒã‚§ãƒƒã‚¯
        return name in self.members


# --------------------------------------------------
# å­ã‚¯ãƒ©ã‚¹: å¤šé‡ç¶™æ‰¿ã§4ã¤ã®æ©Ÿèƒ½ã‚’çµ±åˆ
# --------------------------------------------------
class HybridDaifukuShop(ShopBase, OnlineServiceMixin, 
                        DeliveryServiceMixin, MembershipServiceMixin):
    """
    ãƒã‚¤ãƒ–ãƒªãƒƒãƒ‰å¤§ç¦åº—ï¼ˆå¤šé‡ç¶™æ‰¿ï¼‰
    
    ç¶™æ‰¿å…ƒï¼ˆã‚«ãƒƒã‚³å†…ã«è¤‡æ•°ã®è¦ªã‚’ã‚«ãƒ³ãƒåŒºåˆ‡ã‚Šã§åˆ—æŒ™ï¼‰:
    - ShopBaseï¼ˆåº—èˆ—æ©Ÿèƒ½ï¼‰
    - OnlineServiceMixinï¼ˆã‚ªãƒ³ãƒ©ã‚¤ãƒ³æ©Ÿèƒ½ï¼‰
    - DeliveryServiceMixinï¼ˆé…é”æ©Ÿèƒ½ï¼‰
    - MembershipServiceMixinï¼ˆä¼šå“¡æ©Ÿèƒ½ï¼‰
    
    å½¹å‰²: ã™ã¹ã¦ã®æ©Ÿèƒ½ã‚’çµ±åˆã—ãŸæœ€å…ˆç«¯åº—èˆ—
    
    å¤šé‡ç¶™æ‰¿ã®æ§‹æ–‡:
        class å­ã‚¯ãƒ©ã‚¹å(è¦ªA, è¦ªB, è¦ªC, è¦ªD):
    
    æ³¨æ„ç‚¹:
        - å…¨ã¦ã®è¦ªã‚’åˆæœŸåŒ–ã™ã‚‹å¿…è¦ãŒã‚ã‚‹
        - super()ã ã‘ã§ã¯ä¸ååˆ†ï¼ˆè¤‡æ•°è¦ªãªã®ã§ï¼‰
        - è¦ªå.__init__(self, ...) ã®å½¢å¼ã§ç›´æ¥å‘¼ã¶
    """
    
    def __init__(self, owner_name, stock):
        """
        ã‚³ãƒ³ã‚¹ãƒˆãƒ©ã‚¯ã‚¿ï¼ˆå¤šé‡ç¶™æ‰¿ç‰ˆï¼‰
        
        å¼•æ•°:
            owner_name (str): åº—é•·å
            stock (int): åˆæœŸåœ¨åº«
        
        é‡è¦: ã™ã¹ã¦ã®è¦ªã‚¯ãƒ©ã‚¹ã‚’åˆæœŸåŒ–ã™ã‚‹å¿…è¦ãŒã‚ã‚‹ï¼
              ä¸€ã¤ã§ã‚‚å¿˜ã‚Œã‚‹ã¨ãã®æ©Ÿèƒ½ãŒä½¿ãˆãªã„
        """
        # é–‹å§‹ãƒ¡ãƒƒã‚»ãƒ¼ã‚¸
        print(f"\n{'='*60}")
        print(f"ğŸŒŸ ãƒã‚¤ãƒ–ãƒªãƒƒãƒ‰åº—èˆ—ã‚’èµ·å‹•ä¸­...")
        print(f"{'='*60}")
        
        # å„è¦ªã‚¯ãƒ©ã‚¹ã‚’åˆæœŸåŒ–ï¼ˆé †ç•ªã«å‘¼ã³å‡ºã™ï¼‰
        # è¦ªã‚¯ãƒ©ã‚¹å.__init__(self, å¼•æ•°) ã®å½¢å¼
        # super()ã˜ã‚ƒãªãã¦è¦ªå.__init__()ã‚’ä½¿ã†ç†ç”±:
        #   è¤‡æ•°ã®è¦ªãŒã‚ã‚‹ã®ã§ã€ã©ã®è¦ªã‹æ˜ç¤ºã™ã‚‹å¿…è¦ãŒã‚ã‚‹
        
        ShopBase.__init__(self, owner_name, stock)  # è¦ªAåˆæœŸåŒ–
        OnlineServiceMixin.__init__(self)  # è¦ªBåˆæœŸåŒ–
        DeliveryServiceMixin.__init__(self)  # è¦ªCåˆæœŸåŒ–
        MembershipServiceMixin.__init__(self)  # è¦ªDåˆæœŸåŒ–
        
        # å®Œäº†ãƒ¡ãƒƒã‚»ãƒ¼ã‚¸
        print(f"âœ… ã™ã¹ã¦ã®æ©Ÿèƒ½ãŒåˆ©ç”¨å¯èƒ½ã§ã™")
        print(f"{'='*60}\n")
    
    def process_order(self, customer, quantity, address, is_member=False):
        """
        çµ±åˆæ³¨æ–‡å‡¦ç†ï¼ˆã™ã¹ã¦ã®æ©Ÿèƒ½ã‚’çµ„ã¿åˆã‚ã›ï¼‰
        
        å‡¦ç†ã®æµã‚Œ:
        1. ã‚ªãƒ³ãƒ©ã‚¤ãƒ³æ³¨æ–‡å—ä»˜ï¼ˆè¦ªBã®æ©Ÿèƒ½ï¼‰
        2. è²©å£²å‡¦ç†ï¼ˆè¦ªAã®æ©Ÿèƒ½ï¼‰
        3. é…é”å‡¦ç†ï¼ˆè¦ªCã®æ©Ÿèƒ½ï¼‰
        4. ä¼šå“¡ç™»éŒ²ï¼ˆè¦ªDã®æ©Ÿèƒ½ï¼‰
        
        å¼•æ•°:
            customer (str): é¡§å®¢å
            quantity (int): æ³¨æ–‡å€‹æ•°
            address (str): é…é”å…ˆä½æ‰€
            is_member (bool): ä¼šå“¡ã‹ã©ã†ã‹ï¼ˆãƒ‡ãƒ•ã‚©ãƒ«ãƒˆFalseï¼‰
        
        ãƒ‡ãƒ•ã‚©ãƒ«ãƒˆå¼•æ•°: is_member=False
            å‘¼ã³å‡ºã—æ™‚ã«çœç•¥ã™ã‚‹ã¨ False ãŒä½¿ã‚ã‚Œã‚‹
        """
        # å‡¦ç†é–‹å§‹ãƒ¡ãƒƒã‚»ãƒ¼ã‚¸
        print(f"\n{'='*60}")
        print(f"ğŸ“ æ³¨æ–‡å‡¦ç†é–‹å§‹: {customer}æ§˜")
        print(f"{'='*60}")
        
        # 1. ã‚ªãƒ³ãƒ©ã‚¤ãƒ³æ³¨æ–‡å—ä»˜ï¼ˆè¦ªBã®ãƒ¡ã‚½ãƒƒãƒ‰ï¼‰
        # self.receive_online_order() = è¦ªBã‹ã‚‰ç¶™æ‰¿ã—ãŸãƒ¡ã‚½ãƒƒãƒ‰
        self.receive_online_order(customer, quantity)
        
        # 2. è²©å£²å‡¦ç†ï¼ˆè¦ªAã®ãƒ¡ã‚½ãƒƒãƒ‰ï¼‰
        # self.sell() = è¦ªAã‹ã‚‰ç¶™æ‰¿ã—ãŸãƒ¡ã‚½ãƒƒãƒ‰
        if self.sell(quantity):
            # è²©å£²æˆåŠŸã®å ´åˆ
            
            # 3. é…é”å‡¦ç†ï¼ˆè¦ªCã®ãƒ¡ã‚½ãƒƒãƒ‰ï¼‰
            # self.deliver() = è¦ªCã‹ã‚‰ç¶™æ‰¿ã—ãŸãƒ¡ã‚½ãƒƒãƒ‰
            self.deliver(address)
            
            # 4. ä¼šå“¡ç™»éŒ²å‡¦ç†ï¼ˆè¦ªDã®ãƒ¡ã‚½ãƒƒãƒ‰ï¼‰
            # is_memberãŒTrueã§ã€ã¾ã ç™»éŒ²ã•ã‚Œã¦ã„ãªã„å ´åˆ
            # andæ¼”ç®—å­ = ä¸¡æ–¹Trueãªã‚‰å®Ÿè¡Œ
            # notæ¼”ç®—å­ = Trueã®å¦å®šï¼ˆFalseã«ã™ã‚‹ï¼‰
            if is_member and not self.is_member(customer):
                # self.register_member() = è¦ªDã‹ã‚‰ç¶™æ‰¿ã—ãŸãƒ¡ã‚½ãƒƒãƒ‰
                self.register_member(customer)
            
            # æˆåŠŸãƒ¡ãƒƒã‚»ãƒ¼ã‚¸
            print(f"âœ… æ³¨æ–‡å‡¦ç†å®Œäº†")
        else:
            # è²©å£²å¤±æ•—ã®å ´åˆ
            print(f"âŒ æ³¨æ–‡å‡¦ç†å¤±æ•—")
        
        # å‡¦ç†çµ‚äº†ãƒ¡ãƒƒã‚»ãƒ¼ã‚¸
        print(f"{'='*60}\n")
    
    def show_all_stats(self):
        """
        ã™ã¹ã¦ã®çµ±è¨ˆæƒ…å ±ã‚’è¡¨ç¤º
        
        å‡¦ç†:
            4ã¤ã®è¦ªã‹ã‚‰ç¶™æ‰¿ã—ãŸå…¨å¤‰æ•°ã‚’è¡¨ç¤º
        """
        print(f"\n{'='*60}")
        print(f"ğŸ“Š ãƒã‚¤ãƒ–ãƒªãƒƒãƒ‰åº—èˆ— ç·åˆçµ±è¨ˆ")
        print(f"{'='*60}")
        
        # åº—èˆ—æƒ…å ±ï¼ˆè¦ªAã‹ã‚‰ç¶™æ‰¿ã—ãŸå¤‰æ•°ï¼‰
        print(f"ğŸª åº—é•·: {self.owner_name}")
        print(f"ğŸ“¦ åœ¨åº«: {self.stock}å€‹")
        print(f"ğŸ’° ç´¯è¨ˆè²©å£²: {self.sold}å€‹")
        
        # ã‚ªãƒ³ãƒ©ã‚¤ãƒ³æƒ…å ±ï¼ˆè¦ªBã‹ã‚‰ç¶™æ‰¿ã—ãŸå¤‰æ•°ï¼‰
        print(f"ğŸŒ ã‚ªãƒ³ãƒ©ã‚¤ãƒ³æ³¨æ–‡: {self.online_orders}ä»¶")
        
        # é…é”æƒ…å ±ï¼ˆè¦ªCã‹ã‚‰ç¶™æ‰¿ã—ãŸå¤‰æ•°ï¼‰
        print(f"ğŸšš é…é”å›æ•°: {self.deliveries}å›")
        
        # ä¼šå“¡æƒ…å ±ï¼ˆè¦ªDã‹ã‚‰ç¶™æ‰¿ã—ãŸå¤‰æ•°ï¼‰
        print(f"ğŸ’³ ä¼šå“¡æ•°: {len(self.members)}å")
        
        # åŒºåˆ‡ã‚Šç·š
        print(f"{'='*60}\n")


# --------------------------------------------------
# å¤šé‡ç¶™æ‰¿ã‚’ä½¿ã£ã¦ã¿ã‚‹
# --------------------------------------------------
print("\nã€ãƒ‡ãƒ¢4ã€‘å¤šé‡ç¶™æ‰¿ - ãƒã‚¤ãƒ–ãƒªãƒƒãƒ‰åº—èˆ—")
print("-"*70)
hybrid = HybridDaifukuShop("ãµã‚ãµã‚", 50)



