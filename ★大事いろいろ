rgb = (255, 0, 0)
person = ("å¤ªéƒ", 25, "æ±äº¬")

# 1è¦ç´ ã®ã‚¿ãƒ—ãƒ«ï¼ˆã‚«ãƒ³ãƒãŒå¿…è¦ï¼ï¼‰
single = (1,)  # âœ… ã‚¿ãƒ—ãƒ«
not_tuple = (1)  # âŒ ã“ã‚Œã¯æ•´æ•°ã®1

# ç©ºã®ã‚¿ãƒ—ãƒ«
empty = ()
empty = tuple()

# ã‚«ãƒƒã‚³ãªã—ã§ã‚‚ä½œæˆå¯èƒ½
